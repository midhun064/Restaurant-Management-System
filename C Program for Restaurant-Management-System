int
main ()
{
  printf ("*********************************************************\n");
  printf ("*************** RESTAURANT MANAGEMENT SYSTEM *****************\n");
  printf ("*********************************************************\n");
  printf ("Restaurant Location\n");
  printf ("*****************\n");
  printf ("1-KARUR\n2-NAMMAKAL\n3-TRICHY\n4-THENI\n");
  printf ("ENTER YOUR LOCATION : ");
  int lo;
  scanf ("%d", &lo);
  switch (lo)
	{
	case 1:
	  fun1 ();
	  break;
	case 2:
	  fun2 ();
	  break;
	case 3:
	  fun3 ();
	  break;
	case 4:
	  fun4 ();
	  break;
	default:
	  printf ("Invalid item number!\n");
	}
}
