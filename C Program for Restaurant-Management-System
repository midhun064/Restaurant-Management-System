	  while (order2 != 0);
	  gst2 = (total2 * 18) / 100;
	  tot2 = gst2 + total2;
	  printf ("--------------------------------------------------------\n");
	  printf ("Total quantaity:  %d\n", totalQuantity2);
	  printf ("Total amount you need to pay is: b9%d\n", total2);
	  printf ("GST for your order is: b9%d\n", gst2);
	  printf ("Over all total amount with GST is: b9%d\n", tot2);
	  printf ("--------------------------------------------------------\n");
	}
  else if (dhar == 2)
	{
	  printf
		("NON VEG MENUS ARE : \n 1-MUTTON BIRYANI\n 2-CHIKAN BIRYANI\n 3-FISH BIRYANI\n 4-CHIKAN 65\n");
