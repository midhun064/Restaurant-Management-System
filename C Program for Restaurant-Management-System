			{
			case 1:
			  total1 += 100 * q1;
			  break;
			case 2:
			  total1 += 200 * q1;
			  break;
			case 3:
			  total1 += 150 * q1;
			  break;
			case 4:
			  total1 += 100 * q1;
			  break;
			default:
			  printf ("Invalid item number!\n");
			}
		}
	  while (order1 != 0);
	  gst1 = (total1 * 8) / 100;
	  tot1 = gst1 + total1;
	  printf ("--------------------------------------------------------\n");
