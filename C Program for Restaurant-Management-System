	  int order3, gst3, tot3, total3, totalQuantity3 = 0, q3;
	  do
		{
		  printf ("Enter the dish you want or else enter--0 : ");
		  scanf ("%d", &order3);
		  if (order3 == 0)
			{
			  break;
			}
		  printf ("Please enter the quantity: ");
		  scanf ("%d", &q3);
		  totalQuantity3 += q3;	// Adding to total quantity
		  switch (order3)
			{
			case 1:
			  total3 += 100 * q3;
			  break;
			case 2:
			  total3 += 200 * q3;
			  break;
			case 3:
			  total3 += 150 * q3;
			  break;
			case 4:
			  total3 += 100 * q3;
			  break;
			default:
			  printf ("Invalid item number!\n");
			}
		}
	  while (order3 != 0);
	  gst3 = (total3 * 8) / 100;
	  tot3 = gst3 + total3;
	  printf ("--------------------------------------------------------\n");
	  printf ("Total quantaity:  %d\n", totalQuantity3);
	  printf ("Total amount for your order is: b9%d\n", total3);
	  printf ("GST for your order is: b9%d\n", gst3);
	  printf ("Total amount for your order with GST is: b9%d\n", tot3);
	  printf ("--------------------------------------------------------\n");
	}
  else
	{
	  printf ("Invalid choice!\n");
	}

}

